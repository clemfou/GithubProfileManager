{
  "api_config": {
    "base_url": "https://api.github.com",
    "token": "your_token"
  },
  "logging_config": {
    "disable_existing_loggers": false,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      },
      "custom": {
        "class": "github_profile_manager.utils.logging.CustomFormatter"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "custom",
        "level": "INFO",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "github-profile-manager": {
        "handlers": [
          "console"
        ],
        "level": "INFO",
        "propagate": false
      }
    },
    "root": {
      "handlers": [
        "console"
      ],
      "level": "DEBUG"
    },
    "version": 1
  }
}
